<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="medicineDao" class="org.patientview.repository.radar.dao.alport.impl.MedicineDaoImpl">
        <property name="dataSource" ref="dataSource"/>
        <property name="diseaseGroupDao" ref="diseasGroupDao" />
    </bean>

    <bean id="geneticsDao" class="org.patientview.repository.radar.dao.alport.impl.GeneticsDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="deafnessDao" class="org.patientview.repository.radar.dao.alport.impl.DeafnessDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="hnf1BMiscDao" class="org.patientview.repository.radar.dao.hnf1b.impl.HNF1BMiscDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="issueDao" class="org.patientview.repository.radar.dao.impl.IssueDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="clinicalDataDao" class="org.patientview.repository.radar.dao.impl.ClinicalDataDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="demographicsDao" class="org.patientview.repository.radar.dao.impl.DemographicsDaoImpl">
        <property name="utilityDao" ref="utilityDao"/>
        <property name="dataSource" ref="dataSource"/>
        <property name="diseaseGroupDao" ref="diseasGroupDao"/>
        <property name="genericDiagnosisDao" ref="genericDiagnosisDao"/>
        <property name="userDao" ref="userDao"/>
    </bean>

    <bean id="diagnosisDao" class="org.patientview.repository.radar.dao.impl.DiagnosisDaoImpl">
        <property name="utilityDao" ref="utilityDao"/>
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="hospitalisationDao" class="org.patientview.repository.radar.dao.impl.HospitalisationDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="immunosuppressionDao" class="org.patientview.repository.radar.dao.impl.ImmunosuppressionDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="labDataDao" class="org.patientview.repository.radar.dao.impl.LabDataDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="pathologyDao" class="org.patientview.repository.radar.dao.impl.PathologyDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="plasmapheresisDao" class="org.patientview.repository.radar.dao.impl.PlasmapheresisDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="relapseDao" class="org.patientview.repository.radar.dao.impl.RelapseDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="therapyDao" class="org.patientview.repository.radar.dao.impl.TherapyDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="transplantDao" class="org.patientview.repository.radar.dao.impl.TransplantDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="treatmentDao" class="org.patientview.repository.radar.dao.impl.TreatmentDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="utilityDao" class="org.patientview.repository.radar.dao.impl.UtilityDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="userDao" class="org.patientview.repository.radar.dao.impl.UserDaoImpl">
        <property name="utilityDao" ref="utilityDao"/>
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <!-- radarv generic daos -->
    <bean id="diseasGroupDao" class="org.patientview.repository.radar.dao.impl.DiseaseGroupDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="genericDiagnosisDao" class="org.patientview.repository.radar.dao.impl.GenericDiagnosisDaoImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="medicalResultDao" class="org.patientview.repository.radar.dao.impl.MedicalResultDaoImpl">
        <property name="dataSource" ref="dataSource"/>
        <property name="diseaseGroupDao" ref="diseasGroupDao"/>
    </bean>


</beans>